<script>
import { strTrim } from "@/utils";
import { getPatientDataByPage } from "@/api/deliveryCompany";
import print from "vue-print-nb";

export default {
  name: "DeliveryCompany",
  dicts: ["delivery_person_type"],
  directives: {
    print,
  },
  data() {
    return {
      loading: false,
      dateRange: [],
      listData: [],
      listQuery: {
        pageNo: 1,
        pageSize: 10,
      },
      total: 0,
    };
  },
  created() {
    this.getData();
  },
  mounted() {},

  methods: {
    //获取标准列表信息
    getData() {
      this.loading = true;
      getPatientDataByPage(this.listQuery).then((res) => {
        this.listData = res.data.records;
        this.total = res.data.total;
        this.loading = false;
      });
    },

    //查询
    queryData() {
      this.listQuery.pageNo = 1;
      if (this.dateRange.length > 0) {
        this.listQuery.startTime = this.dateRange[0];
        this.listQuery.endTime = this.dateRange[1];
      }
      this.getData();
    },
    //清空
    resetEmpty() {
      this.dateRange = [];
      // 清空所有的值
      this.listQuery = {
        pageNo: 1,
        pageSize: 10,
      };
      this.getData();
    },
  },
};
</script>
