package com.dincher.project.system.domain.entity;

import com.baomidou.mybatisplus.annotation.*;
import com.dincher.framework.aspectj.lang.annotation.Excel;
import com.dincher.framework.aspectj.lang.annotation.Excel.ColumnType;
import com.dincher.framework.web.domain.CommonEntity;
import com.fasterxml.jackson.annotation.JsonFormat;
import io.swagger.annotations.ApiModelProperty;
import lombok.Data;

import javax.validation.constraints.NotBlank;
import javax.validation.constraints.Size;
import java.util.Date;

/**
 * 患者身份核对记录表
 */
@Data
@TableName("patient_records")
public class PatientRecords extends CommonEntity
{


    /** 角色ID */
    private Integer Id;

    //门诊/急诊/住院/体检号
    private String number;

    //类型 字典表 1门诊 2急诊 3住院 4体检
    private String type;

    //患者姓名
    private String personName;

    //性别
    private String sex;

    //年龄
    private String age;

    //申请科室名称
    private String rpt_dept;

    //检查名称
    private String proj_name;

    //检查部位描述
    private String ckpt_name;

    //检查科室代码
    private String ckdpt_code;

    //检查科室名称
    private String ckdpt_name;

    //检查地点
    private String pos_name;

    //金额
    private String cost;

    //预约时间
    private String appoint_time;

    //核对时间
    private String checkTime;

    //申请备注
    private String app_remark;

    //患者信息核实科室代码
    private String pat_verify_dept_code;

    //患者信息核实科室名称
    private String pat_verify_dept_name;

    //患者信息核实技师工号
    private String pat_verify_code;

    //患者信息核实技师姓名
    private String pat_verify_name;

    //患者信息核实时间
    private String pat_verify_time;

    //状态 确认接口调用状态  1成功
    private String status;





    private String createUser;
    private String updateUser;
    @JsonFormat(pattern = "yyyy-MM-dd HH:mm:ss", timezone = "GMT+8")
    private Date createTime;
    @JsonFormat(pattern = "yyyy-MM-dd HH:mm:ss", timezone = "GMT+8")
    private Date updateTime;

    //@Comment("是否删除 0:正常  1：删除")
    private Integer isDel;


}
